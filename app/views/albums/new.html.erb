<h1>Create Album</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@album) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :user_id %>
      <%= f.text_field :user_id, class: 'form-control' %>

      <%= f.label :title %>
      <%= f.text_field :title, class: 'form-control' %>

      <%= f.submit "Create", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
